{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API": "https://api-dev.ksshub.com"
      }
    },
    "internal-testing": {
      "ios": {
        "enterpriseProvisioning": "adhoc"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API": "https://api-dev.ksshub.com"
      },
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "enterpriseProvisioning": "adhoc"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API": "https://api.ksshub.com"
      },
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
